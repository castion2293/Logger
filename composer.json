            "Pharaoh\\Logger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaoh\\Logger\\LoggerProvider"
            ],
            "aliases": {
                "Logger": "Pharaoh\\Logger\\Facades\\Logger"
            }
        }
    }
}
